<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kenshuu">
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Kenshuu"
        android:name=".ui.base.BaseApplication"
        android:usesCleartextTraffic="true">
        <activity android:name=".ui.main.MainActivity">
        </activity>
        <activity android:name=".ui.total.TotalActivity"/>
        <activity android:name=".ui.user.read.ViewUserActivity"/>
        <activity android:name=".ui.user.create.CreateUserActivity"/>
        <activity android:name=".ui.success.SuccessActivity"/>
        <activity android:name=".ui.user.delete.DeleteUserActivity"/>
        <activity android:name=".ui.user.update.UpdateUserActivity"/>
        <activity android:name=".ui.login.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        <activity
            android:name=".ui.base.BaseActivity"
            android:theme="@style/AppTheme.NoActionBar"
            />
    </application>

</manifest>